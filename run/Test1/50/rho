FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "50";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];


internalField   uniform 1.2041;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform 1.2041;
    }
    top
    {
        type            fixedValue;
        value           uniform 1.2041;
    }
    sidesX
    {
        type            empty;
    }
    sidesY
    {
        type            empty;
    }
}


