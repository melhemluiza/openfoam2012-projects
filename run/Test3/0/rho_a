FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho_a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform 0.96328;
    }

    top
    {
        type            fixedValue;
        value           uniform 0.24082;
    }

    sidesX
    {
        type            empty;
    }

    sidesY
    {
        type            empty;
    }
}

