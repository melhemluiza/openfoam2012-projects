FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "49.5";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 3.856e-06);

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 3.856e-06);
    }
    top
    {
        type            fixedValue;
        value           uniform (0 0 3.856e-06);
    }
    sidesX
    {
        type            empty;
    }
    sidesY
    {
        type            empty;
    }
}


