FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.0000107437883896776);

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0.0000107437883896776);
    }

    top
    {
        type            fixedValue;
        value           uniform (0 0 0.0000107437883896776);
    }

    sidesX
    {
        type            empty;
    }

    sidesY
    {
        type            empty;
    }
}

