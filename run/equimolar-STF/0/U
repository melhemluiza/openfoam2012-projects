FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.0000058218433390);

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform (0 0 0.0000058218433390);
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0.0000058218433390);
    }
    sidesX
    {
        type            empty;
    }
    sidesY
    {
        type            empty;
    }
}

